<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.HomeFragment">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="275dp"
        android:layout_height="100dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="53dp"
        android:fontFamily="@font/poppins"
        android:text="@string/textHome2"
        android:textAlignment="center"
        android:textColor="@color/blueMain"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <Button
        android:id="@+id/btnOrdenaYa"
        style="@style/MiEstiloDeBoton"
        android:layout_width="379dp"
        android:layout_height="53dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/poppins_medium"
        android:text="@string/OrderNow"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="171dp"
        android:layout_height="34dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="81dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="@string/textHome1"
        android:textAlignment="textStart"
        android:textColor="@color/blueMain"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView12" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="57dp"
        android:layout_height="31dp"
        android:layout_marginTop="21dp"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/textHomeName"
        android:textColor="@color/blueMain"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/imageView12" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="17dp"
        android:layout_height="31dp"
        android:layout_marginStart="85dp"
        android:layout_marginTop="21dp"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/textHomeNameO"
        android:textColor="@color/orangeMain"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView12" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="176dp"
        android:layout_height="30dp"
        android:layout_marginStart="147dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="148dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="@string/textHome1.2"
        android:textAlignment="center"
        android:textColor="@color/blueMain"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView21" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:layout_marginStart="155dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="156dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17"
        app:srcCompat="@drawable/restaurant" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="82dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="82dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/large" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="226dp"
        android:layout_height="33dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="37dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="@string/textHome3"
        android:textColor="@color/blueMain"
        android:textSize="22sp"
        app:layout_constraintStart_toEndOf="@+id/textView23"
        app:layout_constraintTop_toBottomOf="@+id/btnOrdenaYa" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginTop="37dp"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/textHomeName"
        android:textColor="@color/blueMain"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/textView22"
        app:layout_constraintTop_toBottomOf="@+id/btnOrdenaYa" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="9dp"
        android:layout_height="36dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="37dp"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/oo"
        android:textColor="@color/blueMain"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnOrdenaYa" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="19dp"
        android:layout_height="32dp"
        android:layout_marginTop="37dp"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/o"
        android:textColor="@color/orangeMain"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/btnOrdenaYa" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="309dp"
        android:layout_height="66dp"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="53dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="@string/textHome3.1"
        android:textAlignment="center"
        android:textColor="@color/blueMain"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView25" />

</androidx.constraintlayout.widget.ConstraintLayout>